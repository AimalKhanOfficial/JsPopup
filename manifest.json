{
  "manifest_version": 3,
  "name": "JsPopup",
  "description": "Extension to display a popup and run custom JS script in it",
  "version": "1.0",
  "icons": {
    "16": "images/jspopupicon-16.png",
    "32": "images/jspopupicon-32.png",
    "48": "images/jspopupicon-48.png",
    "128": "images/jspopupicon-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/jspopupicon-128.png"
  }
}